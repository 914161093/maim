      "fontSize": "10px",
      "iconWidth": "24px",
      "spacing": "3px",
      "height": "50px",
      "backgroundColor": "#ffffff",
      "list": [
        {
          "pagePath": "pages/index/index",
          "iconPath": "/static/tabbar_home_unchecked.png",
          "selectedIconPath": "/static/tabbar_home_checked.png",
          "text": "首页"
        },
        {
          "pagePath": "pages/my/my",
          "iconPath": "/static/tabbar_my_unchecked.png",
          "selectedIconPath": "/static/tabbar_my_checked.png",
          "text": "我的"
        }
      ],
      "selectedIndex": 0,
      "shown": true,
      "child": [
        "lauchwebview"
      ],
      "selected": 0
    }
  },
  "launch_path": "__uniappview.html"
}